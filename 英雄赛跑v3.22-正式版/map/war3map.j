function Trig_OnlineActions takes nothing returns nothing
    if ( ( udg_TestMode == false ) ) then
        set udg_I[9]=1
        loop
            exitwhen udg_I[9] > 5
            if ( ( ( GetPlayerController(ConvertedPlayer(udg_I[9])) == MAP_CONTROL_COMPUTER ) or ( GetPlayerSlotState(ConvertedPlayer(udg_I[9])) != PLAYER_SLOT_STATE_PLAYING ) ) ) then
                set udg_T1player=( udg_T1player - 1 )
                set udg_T1BHplayer=( udg_T1BHplayer - 1 )
                if ( ( ( GetPlayerSlotState(ConvertedPlayer(udg_I[9])) == PLAYER_SLOT_STATE_EMPTY ) or ( GetPlayerSlotState(ConvertedPlayer(udg_I[9])) == PLAYER_SLOT_STATE_LEFT ) or ( GetPlayerController(ConvertedPlayer(udg_I[9])) != MAP_CONTROL_USER ) ) ) then
                    set udg_Online[udg_I[9]]=false
                else
                    call DoNothing()
                endif
            else
                call DoNothing()
            endif
            set udg_I[9]=udg_I[9] + 1
        endloop
        set udg_I[9]=7
        loop
            exitwhen udg_I[9] > 11
            if ( ( ( GetPlayerController(ConvertedPlayer(udg_I[9])) == MAP_CONTROL_COMPUTER ) or ( GetPlayerSlotState(ConvertedPlayer(udg_I[9])) != PLAYER_SLOT_STATE_PLAYING ) ) ) then
                set udg_T2player=( udg_T2player - 1 )
                set udg_T2BHplayer=( udg_T2BHplayer - 1 )
                if ( ( ( GetPlayerSlotState(ConvertedPlayer(udg_I[9])) == PLAYER_SLOT_STATE_EMPTY ) or ( GetPlayerSlotState(ConvertedPlayer(udg_I[9])) == PLAYER_SLOT_STATE_LEFT ) or ( GetPlayerController(ConvertedPlayer(udg_I[9])) != MAP_CONTROL_USER ) ) ) then
                    set udg_Online[udg_I[9]]=false
                else
                    call DoNothing()
                endif
            else
                call DoNothing()
            endif
            set udg_I[9]=udg_I[9] + 1
        endloop
    else
        call DoNothing()
    endif
    // Âà§Êñ≠Áé©ÂÆ∂ÊòØÂê¶Âú®Á∫ø
    call DisableTrigger(gg_trg_leaves_01)
    call EnableTrigger(gg_trg_leaves)
    // ÂàáÊç¢Áé©ÂÆ∂Á¶ªÂºÄÁõ∏ÂÖ≥Ëß¶Âèë
    call DestroyTrigger(GetTriggeringTrigger())
endfunction
