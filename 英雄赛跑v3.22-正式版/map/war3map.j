        function s__ModSpeed_modifyAmount takes integer this,real amount returns nothing
            if amount <= 522 then
                call SetUnitTimeScale(s__ModSpeed_u[this], 1)
                call sc__ModSpeed_destroy(this)
            else
                set s__ModSpeed_bonus[this]=( amount - 522 ) * MaxSpeed___PERIOD
            endif
            set s__ModSpeed_setspeed[this]=amount
        endfunction
        
        
        function s__ModSpeed_register takes unit u returns integer
            local integer this= s__ModSpeed_recycle[(0)]
            if ( 0 == this ) then
                set this=s__ModSpeed_ic + 1
                set s__ModSpeed_ic=this
            else
                set s__ModSpeed_recycle[(0)]=s__ModSpeed_recycle[this]
            endif
            set s__ModSpeed_prev[s__ModSpeed_next[(0)]]=this
            set s__ModSpeed_next[this]=s__ModSpeed_next[(0)]
            set s__ModSpeed_next[(0)]=this
            set s__ModSpeed_prev[this]=(0)
            set s__ModSpeed_u[this]=u
            set s__ModSpeed_bonus[this]=0.
            set s__ModSpeed_x[this]=GetUnitX(u)
            set s__ModSpeed_y[this]=GetUnitY(u)
            return this
        endfunction
        
        function s__ModSpeed__staticgetindex takes unit u returns integer
            local integer this= (0)
            loop
                set this=s__ModSpeed_next[this]
                exitwhen this == 0
                if s__ModSpeed_u[this] == u then
                    return this
                endif
            endloop
            return s__ModSpeed_register(u)
        endfunction
        function s__ModSpeed_move takes integer t returns nothing
            local integer this=t
            if IsUnitType(s__ModSpeed_u[this], UNIT_TYPE_DEAD) then
                call sc__ModSpeed_destroy(this)
            else
                set s__ModSpeed_tempx=GetUnitX(s__ModSpeed_u[this])
                set s__ModSpeed_tempy=GetUnitY(s__ModSpeed_u[this])
                set s__ModSpeed_Incx=s__ModSpeed_tempx - s__ModSpeed_x[this]
                set s__ModSpeed_Incy=s__ModSpeed_tempy - s__ModSpeed_y[this]
                set s__ModSpeed_IncDist=SquareRoot(s__ModSpeed_Incx * s__ModSpeed_Incx + s__ModSpeed_Incy * s__ModSpeed_Incy)
                if s__ModSpeed_IncDist > s__ModSpeed_setspeed[this] * 0.001 and GetUnitAbilityLevel(s__ModSpeed_u[this], 'BSTN') < 1 and GetUnitAbilityLevel(s__ModSpeed_u[this], 'BPSE') < 1 and not IsUnitPaused(s__ModSpeed_u[this]) and s__ModSpeed_setspeed[this] > 522. then
                    call SetUnitTimeScale(s__ModSpeed_u[this], 1 + s__ModSpeed_bonus[this] / 300.)
                    set s__ModSpeed_IncDist=s__ModSpeed_bonus[this] / s__ModSpeed_IncDist
                    set s__ModSpeed_tempx=s__ModSpeed_tempx + s__ModSpeed_Incx * s__ModSpeed_IncDist
                    set s__ModSpeed_tempy=s__ModSpeed_tempy + s__ModSpeed_Incy * s__ModSpeed_IncDist
                    call SetUnitX(s__ModSpeed_u[this], s__ModSpeed_tempx)
                    call SetUnitY(s__ModSpeed_u[this], s__ModSpeed_tempy)
                    set s__ModSpeed_x[this]=s__ModSpeed_tempx
                    set s__ModSpeed_y[this]=s__ModSpeed_tempy
                endif
            endif
        endfunction
            
        function s__ModSpeed_iterate takes nothing returns nothing
            local integer this= (0)
            loop
                set this=s__ModSpeed_next[this]
                exitwhen this == 0
                if IsUnitType(s__ModSpeed_u[this], UNIT_TYPE_DEAD) then
                    call sc__ModSpeed_destroy(this)
                else
                    call s__ModSpeed_move(this)
                endif
            endloop
        endfunction
        
        function s__ModSpeed_destroy takes integer this returns nothing
            set s__ModSpeed_next[s__ModSpeed_prev[this]]=s__ModSpeed_next[this]
            set s__ModSpeed_prev[s__ModSpeed_next[this]]=s__ModSpeed_prev[this]
            set s__ModSpeed_recycle[this]=s__ModSpeed_recycle[(0)]
            set s__ModSpeed_setspeed[this]=0.
            set s__ModSpeed_recycle[(0)]=this
        endfunction
        
//Implemented from module MaxSpeed___O:
        function s__ModSpeed_MaxSpeed___O___onInit takes nothing returns nothing
            call TimerStart(CreateTimer(), MaxSpeed___PERIOD, true, function s__ModSpeed_iterate)
        endfunction
    
        function GetUnitMoveSpeedEx takes unit u returns real
            return s__ModSpeed_setspeed[s__ModSpeed__staticgetindex(u)]
        endfunction
        
        function SetUnitMoveSpeedEx takes unit u,real speed returns nothing
            call s__ModSpeed_modifyAmount(s__ModSpeed__staticgetindex(u),speed)
        endfunction
        
